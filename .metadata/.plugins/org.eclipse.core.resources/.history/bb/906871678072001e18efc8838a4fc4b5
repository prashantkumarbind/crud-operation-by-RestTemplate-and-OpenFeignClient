import org.springframework.web.bind.annotation.RestController;

@RestController
public class EmployeeController {

	  @GetMapping("/testing/{name}")
	  public String check(@PathVariable String name) {
		     return  "Your Input Name Is The::"+name;
	  }
	  
	  
}
